{
  "name": "my-data",
  "private": true,
  "version": "0.1.0",
  "description": "This is the project for the 2023 Inatel's Hackathon. My Data is intended to observe data usage",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
